    "main": "server/app.js",
    "scripts": {
        "clean": "rm -rf build",
        "build": "npm run build:server && npm run build:client",
        "build:server": "NODE_ENV=production tsc",
        "build:client": "webpack -p --progress --config webpack-prod.config",
        "start": "concurrently --raw \"npm run dev:server\" \"npm run dev:client\" ",
        "dev:server": "nodemon",
        "start:production": "NODE_ENV=production node ./build/server/app.js",
        "pretest": "npm run lint",
        "webpack-dev-server": "webpack-dev-server --config webpack-dev.config",
        "dev:client": "webpack --watch --config webpack-dev.config"
    },
